<%= form_for(@instrument) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :category %>
  <%= f.select :category, ['String', 'Brass', 'Percussion', 'Guitar', 'other'] %>
  <%= f.label :picture %>
  <%= f.file_field :picture, accept: 'image/jpeg,image/gif,image/png' %>
  <%= f.submit %>
<% end %>
